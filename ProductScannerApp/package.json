{
  "name": "ProductScannerApp",
  "version": "0.0.1",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint": "tsc && eslint .",
    "clean": "rm -rf node_modules/ ios/Pods Podfile.lock",
    "postinstall": "yarn setup-scandit && yarn install-pods",
    "setup-scandit": "rm -rf scandit-barcodescanner-ios_5.14.2/ && unzip scandit-barcodescanner-ios_5.14.2.zip && mkdir node_modules/scandit-react-native/ios/ScanditBarcodeScanner/Frameworks && mv scandit-barcodescanner-ios_5.14.2/ScanditSDK/ScanditBarcodeScanner.framework node_modules/scandit-react-native/ios/ScanditBarcodeScanner/Frameworks/ScanditBarcodeScanner.framework && rm -rf scandit-barcodescanner-ios_5.14.2/",
    "install-pods": "cd ios && pod install"
  },
  "dependencies": {
    "@types/react": "^16.9.23",
    "axios": "0.19.2",
    "buffer": "5.5.0",
    "cheerio": "0.22.0",
    "events": "3.1.0",
    "ramda": "0.27.0",
    "react": "16.13.0",
    "react-native": "0.61.5",
    "react-native-bootsplash": "2.0.5",
    "react-native-dotenv": "0.2.0",
    "react-native-tailwindcss": "1.1.9",
    "scandit-react-native": "5.14.2",
    "stream": "0.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@babel/runtime": "7.8.7",
    "@react-native-community/eslint-config": "0.0.7",
    "@types/cheerio": "^0.22.16",
    "@types/ramda": "^0.26.43",
    "@types/react": "16.9.23",
    "@types/react-native": "0.61.21",
    "@types/react-test-renderer": "16.9.2",
    "babel-jest": "25.1.0",
    "intermock": "^0.2.2",
    "metro-react-native-babel-preset": "0.58.0",
    "react-test-renderer": "16.13.0",
    "ts-dev-common": "1.0.0"
  }
}
